    <versions>
      <version flow_version="2.0" tools_min_version="3.0.0">
        <num>4.0.1 beta2 release</num>
        <commit>release-v4.0.1-Beta2</commit>
        <desc>4.0.1 beta2 release</desc>
      </version>
    </versions>
  </middleware>
</middleware>
